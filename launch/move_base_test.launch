<launch>


      <remap from="/cmd_vel" to="/nav_cmd_vel"/>
     <!-- <remap from="openni_points" to="/planner_cloud"/> -->
     
      <!--<remap from="move_base_simple/goal" to="/planner_goal"/> -->
        
      <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
         <rosparam file="$(find rosmower)/conf/nav_test/costmap_common_params.yaml" command="load" ns="global_costmap" />
         <rosparam file="$(find rosmower)/conf/nav_test/costmap_common_params.yaml" command="load" ns="local_costmap" />
         <rosparam file="$(find rosmower)/conf/nav_test/local_costmap_params.yaml" command="load" />
         <rosparam file="$(find rosmower)/conf/nav_test/global_costmap_params.yaml" command="load" />
         <rosparam file="$(find rosmower)/conf/nav_test/base_local_planner_params.yaml" command="load" />
      </node>


</launch>